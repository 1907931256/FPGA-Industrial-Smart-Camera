QSlider::groove:horizontal {
border: 1px solid #4A708B;
background: #C0C0C0;
height: 5px;
border-radius: 1px;
padding-left:-1px;
padding-right:-1px;
}

QSlider::sub-page:horizontal {
background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
    stop:0 rgb(255, 224, 142), stop:1 rgb(255, 214, 102));
background: qlineargradient(x1: 0, y1: 0.2, x2: 1, y2: 1,
    stop:0 rgb(250, 210, 85), stop:1 rgb(144, 182, 54));
border: 1px solid #4A708B;
height: 10px;
border-radius: 2px;
}

QSlider::add-page:horizontal {
background: #575757;
border: 0px solid #777;
height: 20px;
border-radius: 2px;
}

QSlider::handle:horizontal
{
    background: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5,
    stop:0.6 rgb(163, 212, 118), stop:0.778409 rgb(255, 255, 255));

    height: 25px;
    width: 30px;
    margin-top: -6px;
    margin-bottom: -6px;
    border-radius: 8Px;
}

