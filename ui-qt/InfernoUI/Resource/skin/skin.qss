QListView::item:selected:!active {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgb(255, 224, 142), stop:0.5 rgb(255, 215, 102), stop:1 rgb(254, 246, 192));
    border-width: 1px;
    border-color: rgb(211, 132, 56);
    border-style: solid;
    border-radius: 3;
    color:rgb(0, 0, 0);
}

QListView::item:selected:active {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgb(249, 220, 117), stop:0.5 rgb(244, 201, 69), stop:1 rgb(249, 220, 117));
    border-width: 1px;
    border-color: rgb(211, 132, 56);
    border-style: solid;
    border-radius: 3;
    color:rgb(0, 0, 0);
}

